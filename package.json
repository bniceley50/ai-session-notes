{
    "name": "ai-session-notes",
    "version": "0.1.0",
    "private": true,
    "packageManager": "pnpm@10.28.0",
    "engines": {
        "node": ">=22"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit",
        "test": "tsx --test --import ./tests/setup-env.ts \"src/**/*.test.ts\"",
        "test:jobs": "tsx --test --import ./tests/setup-env.ts src/lib/jobs/*.test.ts",
        "test:watch": "tsx --test --watch --import ./tests/setup-env.ts",
        "hygiene:audit": "node tools/hygiene-audit.mjs --mode local --out-dir .hygiene --fail-on P1",
        "hygiene:audit:deep": "node tools/hygiene-audit.mjs --mode deep --out-dir .hygiene --fail-on NONE",
        "hygiene:autofix": "node tools/hygiene-autofix.mjs",
        "hygiene:openclaw:summary": "node tools/hygiene-openclaw-summary.mjs bniceley50/ai-session-notes .hygiene/openclaw-summary.md",
        "labels:sync": "node tools/sync-labels.mjs",
        "labels:check": "node tools/validate-labels.mjs",
        "labels:check:strict": "node tools/validate-labels.mjs --strict-sort"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.74.0",
        "@heroicons/react": "^2.2.0",
        "@supabase/ssr": "^0.8.0",
        "@supabase/supabase-js": "^2.91.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "docx": "^9.5.1",
        "jose": "^6.1.3",
        "jspdf": "^4.1.0",
        "lucide-react": "^0.563.0",
        "next": "16.1.6",
        "next-themes": "^0.4.6",
        "openai": "^6.18.0",
        "radix-ui": "^1.4.3",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "sonner": "^2.0.7",
        "tailwind-merge": "^3.4.0"
    },
    "devDependencies": {
        "@playwright/test": "^1.58.2",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "babel-plugin-react-compiler": "1.0.0",
        "eslint": "^9",
        "eslint-config-next": "16.1.2",
        "shadcn": "^3.8.4",
        "tailwindcss": "^4",
        "tsx": "^4.21.0",
        "tw-animate-css": "^1.4.0",
        "typescript": "^5"
    }
}

